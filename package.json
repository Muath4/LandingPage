{
  "name": "landingpage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44412 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44412 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "build:ssr": "ng run LandingPage:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0-next.4",
    "@angular/common": "^16.0.0-next.4",
    "@angular/compiler": "^16.0.0-next.4",
    "@angular/core": "^16.0.0-next.4",
    "@angular/forms": "^16.0.0-next.4",
    "@angular/platform-browser": "^16.0.0-next.4",
    "@angular/platform-browser-dynamic": "^16.0.0-next.4",
    "@angular/platform-server": "^16.0.0-next.4",
    "@angular/router": "^16.0.0-next.4",
    "bootstrap": "^5.2.2",
    "chart.js": "^4.2.1",
    "jquery": "^3.6.4",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0-next.5",
    "@angular/cli": "^16.0.0-next.5",
    "@angular/compiler-cli": "^16.0.0-next.4",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "tslib": "^2.5.0",
    "typescript": "~5.0.2"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
